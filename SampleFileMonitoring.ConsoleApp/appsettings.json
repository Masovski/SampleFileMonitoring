{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "SystemPerformanceOptions": {
    "ConcurrencyDelayMultiplier": 5,
    "MaxConcurrentIORequests": 3,
    "MaxDegreeOfParallelism": 3,
    "BulkFileProcessorBatchSize":  3
  },
  "ProcessingEndpointsOptions": {
    "BaseAddress": "http://localhost:3030",
    "RelativeRegisterFileEndpoint": "/register/file",
    "RelativeRegisterSystemEndpointPath": "v2/register/system"
  },
  "MonitoringOptions": {
    "AllowedFileExtensions": [
      ".cache",
      ".txt",
      ".log"
    ]
  }
}
